{
  "name": "frontcore",
  "version": "0.0.0",
  "description": "A web based centralize tool to perform static code analysis for your projects",
  "author": "Ashwin Hegde",
  "scripts": {
    "dev": "NODE_ENV=development babel-node ./server/server.js",
    "start": "NODE_ENV=production babel-node ./server/server.js",
    "nodemon": "nodemon ./server/server.js --exec babel-node --presets es2015",
    "bundle": "webpack -d --watch",
    "build": "webpack -p",
    "test": "mocha && jest --no-cache",
    "coverage": "jest --coverage",
    "nspcheck": "nsp check"
  },
  "dependencies": {
    "babel-relay-plugin": "^0.9.3",
    "body-parser": "^1.15.0",
    "bootstrap": "^3.3.6",
    "brace": "^0.8.0",
    "classnames": "^2.2.3",
    "compression": "^1.5.2",
    "csslint": "^0.10.0",
    "debug": "~2.2.0",
    "elasticsearch": "^11.0.1",
    "es6-promise": "^3.2.1",
    "eslint": "^2.6.0",
    "events": "^1.1.0",
    "express": "^4.13.4",
    "express-graphql": "^0.5.4",
    "font-awesome": "^4.5.0",
    "fuse.js": "^2.5.0",
    "graphql": "^0.7.0",
    "helmet": "^2.1.2",
    "highcharts": "^4.2.5",
    "history": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jsonfile": "^2.3.1",
    "keymirror": "^0.1.1",
    "mongoose": "^4.5.10",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "node-dir": "^0.1.11",
    "nodemailer": "^1.4.0",
    "object-assign": "^4.1.0",
    "passport": "^0.3.0",
    "passport-local": "^1.0.0",
    "react": "^15.1.0",
    "react-ace": "^3.2.0",
    "react-bootstrap": "^0.29.4",
    "react-dom": "^15.1.0",
    "react-dropzone": "^3.5.1",
    "react-redux": "^4.4.5",
    "react-relay": "^0.9.3",
    "react-router": "^2.2.4",
    "react-treebeard": "^1.1.2",
    "react-widgets": "^3.3.1",
    "redux": "^3.4.0",
    "redux-immutable-state-invariant": "^1.2.3",
    "redux-thunk": "^2.1.0",
    "socket.io": "^1.4.5",
    "superagent": "^1.8.3",
    "underscore": "^1.8.3",
    "validator": "^5.7.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.7.4",
    "babel-jest": "^11.0.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-react": "^4.2.3",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.15.0",
    "ignore-styles": "^4.0.0",
    "jest": "^12.1.1",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.0.2",
    "nsp": "^2.6.2",
    "react-addons-test-utils": "^15.0.1",
    "redux-devtools": "^3.2.0",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "preprocessorIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "./styleMock.js"
    },
    "unmockedModulePathPatterns": [
      "./node_modules/react/",
      "./node_modules/react-dom/",
      "./node_modules/react-addons-test-utils/",
      "./node_modules/enzyme/"
    ]
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": "https://github.com/Frontcore/frontcore",
  "bugs": {
    "url": "https://github.com/Frontcore/frontcore/issues"
  },
  "license": "MIT"
}
